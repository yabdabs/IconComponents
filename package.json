{
  "name": "IconComponents",
  "version": "1.0.0",
  "description": "Converts svg icons to react components",
  "main": "./dist/lib/index.js",
  "scripts": {
    "build": "webpack",
    "icons": "rm -rf ./src/lib/IconComponents && npx @svgr/cli -d ./src/lib/IconComponents ./src/temp --ext jsx --no-svgo",
    "svgo": "npx svgo -f ./src/assets/Icons -o ./src/temp --pretty --config=.svgo.yml"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "Abdul Khan",
  "license": "ISC",
  "dependencies": {
    "@svgr/cli": "^4.0.3",
    "@svgr/core": "^4.0.3",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
